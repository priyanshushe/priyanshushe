#include<stdio.h> #include<conio.h> #include<time.h>
int n,v,s[20],d[20],p[20],w[20][20],t[20][20];
int findmin()
{
int i,min; for(i=1;i<=n;i++)
{
if(s[i]= =0)
{
min=i; break;
}
}
for(i=1;i<=n;i++)
{
if(d[i]<d[min]&&s[i]==0) min=i;
}
return min;
}

void prims()
{
int k,u;
for(int i=1;i<=n;i++)
{ s[i]=0;
d[i]=999;
p[i]=0;
}
//s[v]=1;
